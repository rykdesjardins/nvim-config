{
  "suggest.noselect": false,
  "biome.enable": true,
  "typescript.suggest.autoImports": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql"
  ],
  "languageserver": {
    "typescript": {
      "command": "ts_ls",
      "filetypes": ["typescript", "typescriptreact", "javascript", "javascriptreact"]
    }
  }
}
